                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Recovery email sent"
                    },
                    "400": {
                        "description": "Email not sent",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "examples": {
                                    "User not found": {
                                        "value": {
                                            "message": "User does not exists"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
