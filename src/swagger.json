{
    "openapi": "3.0.0",
    "info": {
        "title": "RentX Documentation",
        "description": "This is an API Rent",
        "version": "1.0.0",
        "contact": {
            "email": "dani@rocketseat.com.br"
        }
    },
    "paths": {
        "/sessions": {
            "post": {
                "tags": [
                    "Session"
                ],
                "summary": "Authentication user",
                "description": "Authenticate a user on the platform",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                    "email": "user@email.com.br",
                                    "password": "pass123"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "token": {
                                            "type": "string"
                                        },
                                        "user": {
                                            "type": "object"
                                        },
                                        "refresh_token": {
                                            "type": "object"
                                        }
                                    }
                                },
                                "examples": {
                                    "Successfully authorized": {
                                        "value": {
                                            "token": "eyJhbGciOiJIUzI1NiIsInR5cCI6I",
                                            "user": {
                                                "name": "User name",
                                                "email": "user@email.com.br"
                                            },
                                            "refresh_token": "eyJhbGciOiJIUzI1NiIsInR5cCI6"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Email or password incorrect!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "examples": {
                                    "Incorrect Credentials": {
                                        "value": {
                                            "message": "Email or password incorrect!"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/refresh-token": {
            "post": {
                "tags": [
                    "Session"
                ],
                "summary": "Refresh token",
                "description": "Renewing a user's access by requesting a new token",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "token": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "token": {
                                            "type": "string"
                                        },
                                        "refresh_token": {
                                            "type": "object"
                                        }
                                    }
                                },
                                "examples": {
                                    "New generated token": {
                                        "value": {
                                            "token": "eyJhbGciOiJIUzI1NiIsInR5cCI6I",
                                            "refresh_token": "eyJhbGciOiJIUzI1NiIsInR5cCI6"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Invalid Token",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "examples": {
                                    "Incorrect Token": {
                                        "value": {
                                            "message": "Invalid Token"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/users": {
            "post": {
                "tags": [
                    "Account"
                ],
                "summary": "Authentication user",
                "description": "Account creation on the platform",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    },
                                    "driver_license": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                    "name": "User Name",
                                    "email": "user@email.com.br",
                                    "password": "pass123",
                                    "driver_license": "ABC-123"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "User Created Successfully"
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "examples": {
                                    "Incorrect Credentials": {
                                        "value": {
                                            "message": "Email or password incorrect!"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/users/avatar": {
            "patch": {
                "tags": [
                    "Account"
                ],
                "summary": "Upload Avatar",
                "description": "Upload a profile picture",
                "security": [
                    {
                        "bearerAuth": []
                    }
                ],
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "avatar": {
                                        "type": "string",
                                        "format": "binary"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "204": {
                        "description": "Avatar sent"
                    },
                    "401": {
                        "description": "Invalid Token",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "examples": {
                                    "Incorrect Token": {
                                        "value": {
                                            "message": "Invalid Token"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/password/forgot": {
            "post": {
                "tags": [
                    "Account"
                ],
                "summary": "Recover password",
                "description": "Send email for password recovery",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
